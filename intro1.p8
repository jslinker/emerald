pico-8 cartridge // http://www.pico-8.com
version 32
__lua__

#include fades.lua

function _init()
    fadeCounter = 1
    fadeIn = true
    frameCounter = 0
    stars = {
        makeStar(16, 8),
        makeStar(24, 2),
        makeStar(64, 2),
        makeStar(72, 8),
        makeStar(88, 24),
        makeStar(108, 9),
        makeStar(112, 6),
    }
    drips = {
        makeDrip(40, 110),
        makeDrip(60, 110),
        makeDrip(66, 110),
        makeDrip(82, 110),
        makeDrip(98, 110),
        makeDrip(115, 110),
    }
end

function _draw()
    cls()
    if fadeCounter > 0 then
        if fadeIn then
            fadeFromBlack(flr(fadeCounter / 3))
        else
            fadeToBlack(flr(fadeCounter / 3))
        end
    end

    spr(0,0,0,128,128)
    rectfill(0,0,8,8,1)
    rectfill(64,0,88,8,0)
    drawStar()
    drawDrip()
    print("Press any button to continue",12,118,7)
end

function drawStar()
    for star in all(stars) do
        if star.shining != 0 then
            local elapsed = frameCounter - star.shining
            local sprite = 8
            local spd = star.speed
            if elapsed < spd then
                sprite = 8
            elseif elapsed < spd*2 then
                sprite = 9
            elseif elapsed < spd*3 then
                sprite = 10
            elseif elapsed < spd*4 then
                sprite = 9
            elseif elapsed < spd*5 then
                sprite = 8
            else
                star.shining = 0
            end
            spr(sprite,star.x,star.y)
        elseif frameCounter % star.start == 0 then
            star.shining = frameCounter
        end
    end

    frameCounter = flr(max(frameCounter + 1, 0))
end

function drawDrip()
    for drip in all(drips) do
        if drip.y > 128 then
            drip.x = drip.srcX
            drip.y = drip.srcY
            drip.velocity = 0
        elseif drip.velocity != 0 then
            drip.x = drip.x
            drip.y = flr(drip.y + drip.velocity)
            drip.velocity += 0.2
            rectfill(drip.x,drip.y,drip.x + 2,drip.y + 2, 11)
        elseif frameCounter % drip.start == 0 then
            drip.velocity = 0.1
        end
    end
end

function _update()
    if btnp(4) or btnp(5) then fadeCounter = 1 end
    if fadeCounter > 0 then fadeCounter += 1 end
    if fadeCounter > 40 then 
        if fadeIn then
            fadeCounter = -1
            fadeIn = false
            pal()
        else
            load("intro2") 
        end
    end
end

function makeStar(x, y)
    return {
        x=x,
        y=y,
        start=flr(rnd(108)),
        speed=10 + flr(rnd(6)),
        shining=0
    }
end

function makeDrip(x, y)
    return {
        srcX = x,
        srcY = y,
        x=x,
        y=y,
        start=flr(108 + rnd(108)),
        velocity = 0
    }
end

